{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo test"
		},
		{
			"type": "cargo",
			"command": "flamegraph",
			"args": [
				"--bench",
				"performance",
				"--",
				"performance",
				"--exact",
				"--show-output"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo flamegraph --test"
		},
		{
			"type": "cargo",
			"command": "build",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build --release"
		}
	]
}